*{box-sizing:inherit}body{line-height:1.5em;font-family:helvetica neue,Helvetica,Arial,sans-serif;margin:0;background-color:#fdfdfd}h1,h2,h3,h4,h5,h6{font-family:helvetica neue,Helvetica,Arial,sans-serif;margin:0;color:#1f2326}h1{font-size:125px;line-height:129px}h2{font-size:90px;line-height:94px}h3{font-size:30px;line-height:34px;font-weight:lighter;letter-spacing:.5px}small{font-size:.5em;color:#bfbfbf}a{color:blue}.menu{padding:0;margin:0}.menu li{display:inline-block;padding:0 10px}.article-meta{text-decoration:none;background:#eee;padding:5px;border-radius:5px}nav.main-nav{background:#1f2326;display:flex;align-items:center;justify-content:space-between;padding:1.5rem}nav.main-nav .logo{text-decoration:none;font-size:2rem;font-weight:700;color:#fcfe00}nav.main-nav .menu{text-align:center}nav.main-nav .menu a{text-decoration:none;color:#fcfe00;padding-left:1rem}.article-meta,footer{text-align:center}.title{font-size:1.1em}footer a{text-decoration:none}hr{border-style:dashed;color:#ddd}pre{border:1px solid #0f1112;border-radius:5px;padding:1em;overflow-x:auto;background-color:#1f2326}code{color:lime}pre code{background:0 0}img,iframe,video{max-width:100%}main{hyphens:auto}blockquote{background:#f9f9f9;border-left:5px solid #ccc;padding:3px 1em}table{margin:auto;border-top:1px solid #bfbfbf;border-bottom:1px solid #bfbfbf}table thead th{border-bottom:1px solid #ddd}th,td{padding:5px}thead,tfoot,tr:nth-child(even){background:#eee}ul{font-size:20px;line-height:24px;font-weight:lighter;letter-spacing:.5px;list-style-type:none;padding:0}ul li{border-bottom:#bfbfbf solid thin;padding:10px 0}.intro{display:grid;grid-template-columns:repeat(2,1fr);align-items:center;align-items:end}.flex-row{box-sizing:border-box;display:flex;flex:0 1 auto;flex-direction:row;flex-wrap:wrap}.half{flex-basis:50%;max-width:50%}.grid{display:grid;grid-template-columns:repeat(3,1fr);grid-gap:2rem}.spacer{padding:4rem}.highlight{background-color:#fcfe00}.bg-primary{background:#1f2326}.bg-primary *{color:#fff}.bg-light{background-color:#edeff0;padding:40px}.box{border:solid thin #edeff0;padding:2rem}.docs h1{font-size:90px;line-height:94px}.docs h2{font-size:30px;line-height:34px}.docs h3{font-size:24px;line-height:28px;font-weight:lighter;letter-spacing:.5px}.docs .container{display:grid;grid-template-areas:"header header header" "nav content side" "footer footer footer";grid-template-columns:300px minmax(50ch,80ch) auto;grid-template-rows:auto 1fr;column-gap:40px;height:100vh}.docs header{grid-area:header}.docs .tree-nav{grid-area:nav;align-items:start;padding:2rem;background-color:#edeff0}.docs .tree-nav ul li a{font-size:14px;font-weight:700;text-decoration:none;color:#1f2326}.docs .tree-nav ul li ul{margin-left:10px}.docs main{grid-area:content}.docs aside{grid-area:side}.docs footer{grid-area:footer}@media(max-width:768px){.docs .container{grid-template-areas:"header" "nav" "content" "side" "footer";grid-template-columns:1fr;grid-template-rows:auto minmax(75px,auto) 1fr minmax(75px,auto) auto}}